<?xml version="1.0" encoding="utf-8"?>
<section id="ifinclude_command">
  <title>ifinclude command</title>
  <simplesect role="usage" id="gen-000">
    <title>Usage</title>
    <para>
      <literal>ifinclude [--encoding|:e <ulink url="s_enc_string.html">encoding</ulink>] <ulink url="s_filename.html">filename</ulink></literal>
    </para>
  </simplesect>
  <simplesect role="description" id="gen-001"><title>Description</title>
	  <para>
	    Unless the file <ulink url="s_filename.html">filename</ulink> has already
	    been included using either <ulink url="s_include_command.html">include</ulink> of
	    <ulink url="s_ifinclude_command.html">ifinclude</ulink>, load the file and
	    execute it as a XSH2 script.
	  </para>
	  <para>Use <literal>--encoding</literal> or <literal>:e</literal>
	    parameter to specify character encoding used in
	    the included file.
	  </para>
	</simplesect>
  <simplesect role="seealso" id="gen-002">
    <title>See Also</title>
    <para>
      <ulink url="s_include_command.html">include</ulink>
    </para>
  </simplesect>
  <simplesect id="gen-003">
    <title>Sections</title>
    <para>
      <ulink url="s_Flow.html">Flow control</ulink>
    </para>
  </simplesect>
</section>
