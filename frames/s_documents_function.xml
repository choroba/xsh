<?xml version="1.0" encoding="utf-8"?>
<section id="documents_function">
  <title>xsh:documents function</title>
  <simplesect role="usage" id="gen-000">
    <title>Usage</title>
    <para>
      <literal>node-set xsh:documents()</literal>
    </para>
  </simplesect>
  <simplesect role="description" id="gen-001"><title>Description</title>
	  <para>
	    Returns a node-set consisting of the document
	    nodes of all currently open documents.
	  </para>
	</simplesect>
  <simplesect role="seealso" id="gen-002">
    <title>See Also</title>
    <para>
      <ulink url="s_hash_command.html">hash</ulink>
    </para>
  </simplesect>
</section>
