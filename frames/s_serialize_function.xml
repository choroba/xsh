<?xml version="1.0" encoding="utf-8"?>
<section id="serialize_function">
  <title>xsh:serialize function</title>
  <simplesect role="usage" id="gen-000">
    <title>Usage</title>
    <para>
      <literal>string xsh:serialize(node-set N,...)</literal>
    </para>
  </simplesect>
  <simplesect role="description" id="gen-001"><title>Description</title>
	  <para>
	    Serializes nodes of given node-set(s)
	    into XML strings and returns concatenation of
	    those strings.
	  </para>	
	</simplesect>
</section>
