<?xml version="1.0" encoding="utf-8"?>
<section id="test_mode">
  <title>test-mode command</title>
  <simplesect role="usage" id="gen-000">
    <title>Usage</title>
    <para>
      <literal>test-mode</literal>
    </para>
  </simplesect>
  <simplesect role="aliases" id="gen-001">
    <title>Aliases</title>
    <para>
      <literal> test_mode</literal>
    </para>
  </simplesect>
  <simplesect role="description" id="gen-002"><title>Description</title>
	  <para>
	    Switch into a mode in which no commands are actually
	    executed and only command syntax is checked.
	  </para>
	  <para>This is equivalent to setting
	    <literal>$TEST_MODE</literal> variable to 1.</para>
	</simplesect>
  <simplesect role="seealso" id="gen-003">
    <title>See Also</title>
    <para>
      <ulink url="s_run_mode.html">run-mode</ulink>
    </para>
  </simplesect>
  <simplesect id="gen-004">
    <title>Sections</title>
    <para><ulink url="s_Flow.html">Flow control</ulink>, <ulink url="s_Configuration.html">Global settings</ulink></para>
  </simplesect>
</section>
