<?xml version="1.0" encoding="utf-8"?>
<section id="print_enc_command">
  <title>enc command</title>
  <simplesect role="usage" id="gen-000">
    <title>Usage</title>
    <para>
      <literal>enc [<ulink url="s_document.html">document</ulink>]</literal>
    </para>
  </simplesect>
  <simplesect role="description" id="gen-001"><title>Description</title>
	  <para>
	    Print the original document encoding string.
	    If no <ulink url="s_document.html">document</ulink> is given, the current document is
	    used.
	  </para>
	</simplesect>
  <simplesect role="seealso" id="gen-002">
    <title>See Also</title>
    <para>
      <ulink url="s_set_enc_command.html">set-enc</ulink>
    </para>
  </simplesect>
  <simplesect id="gen-003">
    <title>Sections</title>
    <para>
      <ulink url="s_Information.html">Retrieving more information</ulink>
    </para>
  </simplesect>
</section>
