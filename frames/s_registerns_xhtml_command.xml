<?xml version="1.0" encoding="utf-8"?>
<section id="registerns_xhtml_command">
  <title>register-xhtml-namespace command</title>
  <simplesect role="usage" id="gen-000">
    <title>Usage</title>
    <para>
      <literal>register-xhtml-namespace <ulink url="s_exp.html">expression</ulink></literal>
    </para>
  </simplesect>
  <simplesect role="aliases" id="gen-001">
    <title>Aliases</title>
    <para>
      <literal> regns-xhtml</literal>
    </para>
  </simplesect>
  <simplesect role="description" id="gen-002"><title>Description</title>
	  <para>
             Registers a prefix for the XHTML namespace
	    <literal>http://www.w3.org/1999/xhtml</literal>.
             The prefix can later be used in XPath expressions.
	  </para>
	</simplesect>
  <simplesect id="gen-003">
    <title>Sections</title>
    <para><ulink url="s_Namespaces.html">Namespaces in XML and XPath</ulink>, <ulink url="s_Configuration.html">Global settings</ulink></para>
  </simplesect>
</section>
